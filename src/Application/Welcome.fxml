<StackPane fx:id="welcome" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Welcome_Cr">
   <children>
      <AnchorPane onMouseClicked="#Load_Calculator" prefHeight="400.0" prefWidth="300.0">
         <children>
            <Label layoutX="21.0" layoutY="14.0" text="Welcome to Standard Calculator" underline="true">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="description" layoutX="90.0" layoutY="175.0" text="Click to Continue">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</StackPane>
